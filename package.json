{
  "name": "ai-penetration-testing-system",
  "version": "1.0.0",
  "description": "AI全自动渗透测试LLM-Agent系统",
  "main": "api/server.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev:api": "nodemon api/server.ts",
    "dev:frontend": "vite --config vite.config.ts",
    "build": "npm run build:api && npm run build:frontend",
    "build:api": "tsc -p api/tsconfig.json",
    "build:frontend": "vite build",
    "start": "node dist/api/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "penetration-testing",
    "llm",
    "security",
    "cybersecurity",
    "automation",
    "pentest"
  ],
  "author": "AI Security Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
